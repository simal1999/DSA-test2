   "source": [
    "def mysqrt(x):\n",
    "    if x==0:\n",
    "        return 0\n",
    "    a=1\n",
    "    b=x\n",
    "    while a<=b:\n",
    "        m=(a+b)//2\n",
    "        if m**2==x:\n",
    "            return m\n",
    "        elif m**2>b:\n",
    "            b=m-1\n",
    "        else:\n",
    "            a=m+1\n",
    "    return a"
   ]
  },
